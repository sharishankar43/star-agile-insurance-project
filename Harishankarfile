pipeline {
  agent any
    tools{
      maven 'M2_HOME'
    }
   stages{
    stage('Git checkout'){
      steps{
         echo 'This is for cloning the Gitrepo'
         git "git 'https://github.com/sharishankar43/star-agile-insurance-project.git'"
      }
    }
    stage('Create a Package'){
      steps{
         echo "This will create a package using maven"
         sh 'mvn package'
      }
    }
    
   }
}
